groupId=org.apache.maven.doxia-book-maven-plugin
artifactId=doxia-book-maven-plugin
version=master

gitRepo=https://github.com/apache/maven-${artifactId}.git
gitTag=${version}

jdk=8
newline=lf

command="mvn -Papache-release clean package -DskipTests -Dmaven.javadoc.skip -Dgpg.skip -Drat.skip"
buildinfo=target/${artifactId}-*.buildinfo
